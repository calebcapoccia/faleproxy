{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/fetch",
      "methods": ["POST"],
      "dest": "/app.js"
    },
    {
      "src": "/.*",
      "dest": "/app.js"
    },
    {
      "src": "/",
      "dest": "/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
